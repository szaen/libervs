var frm;

function PostOpenForm(){

if (document.forms[0].isNewDocument.value =='Novo'){
salvar();
}
}






// Envia o documento para aprovação
//__________________________________________________________________
function enviarAprovacao(){

var data = new Date();
	with(document.forms[0]){
	//- - - - - - - Valida Data da Ocorrência - - - - - - - -
		
		if(Contato.value == ''){
				alert("The selected vendor has not registered a contact. Please provide the registration");
				return;			
			}
		
		
		if(fctValidarDados() == true){
			if(ValidaDatas(TX_DTOCORRENCIA.value,data) == false){
				alert("The ocurrence date can not be after then the current date");
				return;
			}
			if(TX_DATA_NAO_CHECADA.value != ''){
				if(ValidaDatas(data, TX_DATA_NAO_CHECADA.value) == false){
					alert("The replacement date can not be before then the current date");
					return;
				}
			}
			if(parseInt(TX_QTDTOTAL.value) < parseInt(TX_QTDPROBLEMA.value)){
				alert("The TTL Qty can not be lesser than the NG Qty");
				return;
			}				
			
			if(parseInt(TX_QUANTIDADE_NAO_CHECADA.value) > parseInt(TX_QTDPROBLEMA.value)){
				alert("The Quantity can not be larger than NG Qty.");
				return;			
			}
			
			
			
			var txtmsg = "Are you sure you want to send this Trouble Report to approval now?";
		
			if(confirm(txtmsg)) {
				obs = window.open(dbName.value + '/FOBS?OpenForm&acao=EnviarAprovacao&Fluxo=APROVACAO&documento=TR&ID='+DocUnid.value,'Comments','width=400,height=170');
			}
		}	
}
}
