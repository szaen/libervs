 with(document.forms[0]){ 
   TX_FORCOD.value    = codigo;
   TX_FORNOME.value   = nome;
   TX_FORNIVEL.value  = nivel;
   TX_FORSIGLA.value  = sigla;
   TX_FORNUMERO.value = numero;
   TX_FORANO.value    = ano;
 }
	 salvar_forn();
}

