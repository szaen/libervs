function fctValidarDadosRascunho(){
	var arrayCampos;

	arrayCampos = new Array(6);

	if(document.forms[0].Trouble.value == "Emitido"){
		arrayCampos[0] = "TX_FORCOD";
		arrayCampos[1] = "Data For Issue Section - To";
		arrayCampos[2] = "TX_DESCPECA";
		arrayCampos[3] = "Data Material Section - Part";
		arrayCampos[4] = "TX_DTOCORRENCIA";
		arrayCampos[5] = "Occurrence Information Section - Occurrence Date";
	}else{
		arrayCampos[0] = "TX_DESTINO";
		arrayCampos[1] = "Data For Issue Section - To";
		arrayCampos[2] = "TX_ITEMDESC_ING"; //Trd 2013 16/07/2013
		arrayCampos[3] = "Data Material Section - Part";
		arrayCampos[4] = "TX_DTOCORRENCIA";
		arrayCampos[5] = "Occurrence Information Section - Occurrence Date";	
	}
	

	if( JS_Funcoes_fctValidaCampos( arrayCampos ) == true ){
		return true;
	}else{
		return false;
	}
}

