	 	if(document.forms[0].Trouble.value == "Recebido"){
	 		if(document.forms[0].NUM_ETAPA.value > 1){
		 		arrayCamposDel[0] = "trouble_delegation";					
		 		arrayCamposDel[1] = "delegacao_trouble";
			 	arrayCamposDel[2] = "delegating_for";					
			 	arrayCamposDel[3] = "delegar_para";		
			 	arrayCampos = arrayCampos.concat( arrayCamposDel );
			 	
				for( var j = 0 ; j < frm.TX_DELEGACAO.length ; j++ ){
					if( frm.TX_DELEGACAO[j].selected) {
						if(frm.TX_DELEGACAO[j].text == "Supplier"){
					 		arrayCamposDel[0] = "supplier_delegating";
						 	arrayCamposDel[1] = "delegar_fornecedor";
				 		 	arrayCampos = arrayCampos.concat( arrayCamposDel );
							break;				
						}else if(frm.TX_DELEGACAO[j].text == "Internal Area"){
						 	arrayCamposDel[0] = "internal_area";
						 	arrayCamposDel[1] = "delegar_ccusto";	 	
					 		arrayCampos = arrayCampos.concat( arrayCamposDel );
							break;				
						}				
					}
				}	
			}	
		}		
			if(document.forms[0].Trouble.value == "Emitido"){
			 	arrayCampos3G[0] = "emitente_3g";		
			 	arrayCampos3G[1] = "issuer_3g";
			 	arrayCampos = arrayCampos.concat(arrayCampos3G);			 	
			 }
	 }
	JS_Funcoes_fctMudaIdioma(arrayCampos)	 
}

// Visualizar 3G
//*************************************************************************************
function vis3G(){
	with(document.forms[0]){
		var urlStr = '/'+BASE_RI.value+'/AGOPENRI?OpenAgent&NUMRI='+TX_NUM_3G.value+'&ANORI='+TX_ANO_3G.value+'&FINRI='+TX_FIN_3G.value+'&GERACLI=N&GERAIO=T&VTR=OK';
		//alert(urlStr)
		window.open(urlStr,'Visualizar_3G','toolbar=no,scrollbars=yes,width=900, height=600,resizeble=NO');
	}
}

function selectIt(){
	var tr = parent.document.getElementByID('TR');
	var ri = parent.document.getElementByID('RI');
	if(tr.className != '')
		return false;
	l = tr.parentNode.childNodes.length;
	for(i = 0; i < l;i++)
		tr.parentNode.childNodes[i].className = '';
		tr.className = 'current';
		
	if(ri.className = '')
		return false;
	m = ri.parentNode.childNodes.length;
	for(j = 0; j < m;j++)
		ri.parentNode.childNodes[j].className = '';
		ri.className = '';
}

